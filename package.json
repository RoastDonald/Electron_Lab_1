    "react-redux": "^7.2.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^3.4.0",
    "react-spinners": "^0.8.0",
    "react-transition-group": "^4.3.0",
    "redux": "^4.0.5",
    "redux-form": "^8.3.0",
    "redux-logger": "^3.0.6",
    "redux-persist": "^6.0.0",
